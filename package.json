{
  "name": "double-slash-website",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "start": "node .output/server/index.mjs",
    "lint": "eslint . --ext .ts,.vue",
    "prepare": "husky install",
    "format-check": "npx prettier --check ./",
    "format-write": "npx prettier --write ./",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "generate": "pnpm run build && npx nuxi generate"
  },
  "devDependencies": {
    "@nuxt/content": "^2.2.1",
    "@nuxt/test-utils": "3.0.0-rc.12",
    "@nuxt/test-utils-edge": "3.0.0-rc.8-27673249.1b2304b",
    "@nuxtjs/color-mode": "^3.1.8",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/tailwindcss": "^6.1.2",
    "@pinia/nuxt": "^0.4.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/rss": "^0.0.29",
    "@types/xml2json": "^0.11.4",
    "@vueuse/nuxt": "^9.4.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "nuxt": "3.0.0-rc.12",
    "nuxt-icon": "0.1.7",
    "pinia": "^2.0.23",
    "postcss-custom-properties": "^12.1.10",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sitemap": "^7.1.1",
    "typescript": "^4.8.4",
    "vitest": "^0.24.5",
    "xml2json": "^0.12.0"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "pnpm prettier --write",
      "pnpm lint",
      "pnpm test run related  -- --passWithNoTests"
    ]
  },
  "volta": {
    "node": "16.17.0"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.3",
    "@nathanchase/nuxt-dayjs-module": "^1.0.11",
    "@tailwindcss/forms": "^0.5.3",
    "axios": "^1.1.3",
    "crypto": "^1.0.1",
    "hast-util-to-html": "^8.0.3",
    "motion": "^10.14.3",
    "rss": "^1.2.2",
    "simple-json-db": "^2.0.0",
    "vue-lite-youtube-embed": "^1.0.4"
  }
}
