{
    "name": "double-slash-website",
    "scripts": {
        "plop": "plop",
        "build": "nuxi build",
        "dev": "nuxi dev",
        "start": "node .output/server/index.mjs",
        "lint": "eslint . --ext .ts,.vue",
        "prepare": "husky install",
        "format-check": "npx prettier --check ./",
        "format-write": "npx prettier --write ./",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "generate": "pnpm run build && npx nuxi generate"
    },
    "devDependencies": {
        "@nuxt/content": "2.4.3",
        "@nuxt/image-edge": "1.0.0-27827328.bc9ddc0",
        "@nuxt/test-utils": "3.2.0",
        "@nuxt/test-utils-edge": "3.1.1-rc.0-27911047.7d812db",
        "@nuxthq/studio": "^0.10.0",
        "@nuxtjs/color-mode": "3.2.0",
        "@nuxtjs/eslint-config-typescript": "12.0.0",
        "@nuxtjs/fontaine": "^0.2.4",
        "@nuxtjs/tailwindcss": "6.2.0",
        "@pinia/nuxt": "0.4.6",
        "@tailwindcss/typography": "0.5.9",
        "@types/rss": "0.0.30",
        "@types/xml2json": "0.11.4",
        "@vueuse/nuxt": "9.12.0",
        "eslint": "8.34.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-nuxt": "4.0.0",
        "eslint-plugin-prettier": "4.2.1",
        "husky": "8.0.3",
        "jsdom": "21.1.0",
        "lint-staged": "13.1.1",
        "nuxt": "3.2.0",
        "nuxt-icon": "0.2.11",
        "nuxt-schema-org": "2.1.1",
        "pinia": "2.0.30",
        "plop": "^3.1.2",
        "postcss-custom-properties": "13.1.3",
        "prettier": "2.8.4",
        "prettier-plugin-tailwindcss": "0.2.2",
        "sitemap": "7.1.1",
        "typescript": "4.9.5",
        "vitest": "0.28.4",
        "xml2json": "0.12.0"
    },
    "lint-staged": {
        "*.{js,vue,ts}": [
            "pnpm prettier --write",
            "pnpm lint",
            "pnpm test run related  -- --passWithNoTests"
        ]
    },
    "volta": {
        "node": "16.17.0"
    },
    "dependencies": {
        "@headlessui/vue": "1.7.9",
        "@nathanchase/nuxt-dayjs-module": "1.0.11",
        "@tailwindcss/forms": "0.5.3",
        "axios": "1.2.4",
        "crypto": "1.0.1",
        "motion": "10.15.5",
        "rss": "1.2.2",
        "simple-json-db": "2.0.0",
        "vue-lite-youtube-embed": "1.2.0"
    }
}
