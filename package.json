        "format-write": "npx prettier --write ./",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "generate": "pnpm run build && npx nuxi generate"
    },
    "devDependencies": {
        "@nuxt/content": "2.7.2",
        "@nuxt/image-edge": "1.0.0-rc.1-28180077.d51e794",
