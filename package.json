{
  "name": "double-slash-website",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "start": "node .output/server/index.mjs",
    "lint": "eslint . --ext .ts,.vue",
    "prepare": "husky install",
    "format-check": "npx prettier --check ./",
    "format-write": "npx prettier --write ./",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.7",
    "@iconify-json/twemoji": "^1.1.5",
    "@nuxt/content": "^2.0.1",
    "@nuxt/test-utils": "^3.0.0-rc.8",
    "@nuxt/test-utils-edge": "3.0.0-rc.8-27673249.1b2304b",
    "@nuxtjs/color-mode": "^3.1.4",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/tailwindcss": "^5.3.2",
    "@pinia/nuxt": "^0.4.1",
    "@types/rss": "^0.0.29",
    "@types/xml2json": "^0.11.4",
    "@vueuse/nuxt": "^9.1.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "nuxt": "3.0.0-rc.6",
    "pinia": "^2.0.19",
    "postcss-custom-properties": "^12.1.8",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "typescript": "^4.7.4",
    "vitest": "^0.22.1",
    "xml2json": "^0.12.0"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "yarn lint",
      "yarn format-write",
      "yarn test run related --passWithNoTests"
    ]
  },
  "volta": {
    "node": "16.17.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "crypto": "^1.0.1",
    "hast-util-to-html": "^8.0.3",
    "rss": "^1.2.2"
  }
}
