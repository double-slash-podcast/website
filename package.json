{
  "name": "double-slash-website",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "start": "node .output/server/index.mjs",
    "lint": "eslint . --ext .ts,.vue",
    "prepare": "husky install",
    "format-check": "npx prettier --check ./",
    "format-write": "npx prettier --write ./",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.6",
    "@iconify-json/twemoji": "^1.1.5",
    "@nuxt/content": "^2.0.1",
    "@nuxt/test-utils-edge": "^3.0.0-rc.4-27621410.b045b71",
    "@nuxtjs/color-mode": "^3.1.4",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/tailwindcss": "^5.1.3",
    "@pinia/nuxt": "^0.1.9",
    "@types/xml2json": "^0.11.4",
    "@vueuse/nuxt": "^8.9.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "nuxt": "3.0.0-rc.4",
    "pinia": "^2.0.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "typescript": "^4.7.4",
    "vitest": "^0.18.0",
    "xml2json": "^0.12.0"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "yarn lint",
      "yarn format-write",
      "yarn test run related --passWithNoTests"
    ]
  },
  "volta": {
    "node": "17.9.1"
  }
}
