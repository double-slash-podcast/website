        "@nuxthq/studio": "0.13.4",
        "@nuxtjs/color-mode": "3.3.0",
        "@nuxtjs/eslint-config-typescript": "12.0.0",
        "@nuxtjs/fontaine": "0.4.0",
        "@nuxtjs/tailwindcss": "6.8.0",
        "@pinia/nuxt": "0.4.11",
        "@tailwindcss/typography": "0.5.9",
        "@types/rss": "0.0.30",
        "@types/xml2json": "0.11.4",
        "@vueuse/nuxt": "10.2.1",
        "consola": "^3.2.2",
        "eslint": "8.43.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-nuxt": "4.0.0",
        "eslint-plugin-prettier": "4.2.1",
        "husky": "8.0.3",
        "jsdom": "22.1.0",
        "lint-staged": "13.2.3",
        "nuxt": "3.6.1",
        "nuxt-icon": "0.4.2",
        "nuxt-schema-org": "2.2.0",
        "pinia": "2.1.4",
        "plop": "3.1.2",
        "postcss-custom-properties": "13.3.0",
        "prettier": "2.8.8",
        "prettier-plugin-tailwindcss": "0.3.0",
        "sitemap": "7.1.1",
        "typescript": "5.1.6",
        "vitest": "0.32.2",
        "xml2json": "0.12.0"
    },
    "lint-staged": {
        "*.{js,vue,ts}": [
            "pnpm prettier --write",
            "pnpm lint",
            "pnpm test run related  -- --passWithNoTests"
