{
    "name": "double-slash-website",
    "scripts": {
        "plop": "plop",
        "build": "nuxi build",
        "dev": "nuxi dev",
        "start": "node .output/server/index.mjs",
        "lint": "eslint . --ext .ts,.vue",
        "prepare": "husky install",
        "format-check": "npx prettier --check ./",
        "format-write": "npx prettier --write ./",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "generate": "pnpm run build && npx nuxi generate"
    },
    "devDependencies": {
        "@nuxt/content": "2.5.2",
        "@nuxt/image-edge": "1.0.0-28020728.5df24eb",
        "@nuxt/test-utils": "3.3.3",
        "@nuxt/test-utils-edge": "3.4.3-28033322.607bfdcf",
        "@nuxthq/studio": "0.10.1",
        "@nuxtjs/color-mode": "3.2.0",
        "@nuxtjs/eslint-config-typescript": "12.0.0",
        "@nuxtjs/fontaine": "0.2.5",
        "@nuxtjs/tailwindcss": "6.6.6",
        "@pinia/nuxt": "0.4.9",
        "@tailwindcss/typography": "0.5.9",
        "@types/rss": "0.0.30",
        "@types/xml2json": "0.11.4",
        "@vueuse/nuxt": "10.0.2",
        "eslint": "8.38.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-nuxt": "4.0.0",
        "eslint-plugin-prettier": "4.2.1",
        "husky": "8.0.3",
        "jsdom": "21.1.1",
        "lint-staged": "13.2.0",
        "nuxt": "3.4.2",
        "nuxt-icon": "0.3.3",
        "nuxt-schema-org": "2.1.2",
        "pinia": "2.0.35",
        "plop": "3.1.2",
        "postcss-custom-properties": "13.1.5",
        "prettier": "2.8.7",
        "prettier-plugin-tailwindcss": "0.2.7",
        "sitemap": "7.1.1",
        "typescript": "5.0.4",
        "vitest": "0.30.1",
        "xml2json": "0.12.0"
    },
    "lint-staged": {
        "*.{js,vue,ts}": [
            "pnpm prettier --write",
            "pnpm lint",
            "pnpm test run related  -- --passWithNoTests"
        ]
    },
    "volta": {
