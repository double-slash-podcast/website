    },
    "dependencies": {
        "@headlessui/vue": "1.7.13",
        "@nathanchase/nuxt-dayjs-module": "1.0.15",
        "@tailwindcss/forms": "0.5.3",
        "axios": "1.3.6",
        "crypto": "1.0.1",
        "motion": "10.15.5",
        "rss": "1.2.2",
        "simple-json-db": "2.0.0",
        "vue-lite-youtube-embed": "1.2.0"
    }
