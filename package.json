        "@upstash/redis": "^1.25.1",
        "axios": "1.6.2",
        "crypto": "1.0.1",
        "motion": "10.16.4",
        "rss": "1.2.2",
        "simple-json-db": "2.0.0",
        "vue-lite-youtube-embed": "1.2.1"
    },
    "resolutions": {
        "mlly": "1.4.0"
    }
