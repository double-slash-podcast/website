        "@nuxthq/studio": "1.0.1",
        "@nuxtjs/color-mode": "3.3.0",
        "@nuxtjs/eslint-config-typescript": "12.1.0",
        "@nuxtjs/fontaine": "0.4.1",
        "@nuxtjs/tailwindcss": "6.8.0",
        "@pinia/nuxt": "0.5.0",
        "@tailwindcss/typography": "0.5.10",
        "@types/rss": "0.0.30",
        "@types/xml2json": "0.11.5",
        "@vueuse/nuxt": "10.5.0",
        "consola": "^3.2.3",
        "eslint": "8.51.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-nuxt": "4.0.0",
        "eslint-plugin-prettier": "5.0.1",
        "husky": "8.0.3",
        "jsdom": "22.1.0",
        "lint-staged": "15.0.1",
        "nuxt": "3.7.4",
        "nuxt-icon": "0.5.0",
        "nuxt-schema-org": "3.0.1",
        "pinia": "2.1.7",
        "plop": "4.0.0",
        "postcss-custom-properties": "13.3.2",
        "prettier": "3.0.3",
        "prettier-plugin-tailwindcss": "0.5.6",
        "sitemap": "7.1.1",
        "typescript": "5.2.2",
        "vitest": "0.34.6",
        "xml2json": "0.12.0"
    },
    "lint-staged": {
        "*.{js,vue,ts}": [
            "pnpm prettier --write",
            "pnpm lint",
            "pnpm test run related  -- --passWithNoTests"
